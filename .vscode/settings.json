{
  "python.defaultInterpreterPath": "./.env/Scripts/python.exe",
  "python.analysis.typeCheckingMode": "standard",
  "python.analysis.diagnosticSeverityOverrides": {
    "reportUnusedImport": false
  },
  "python.REPL.enableREPLSmartSend": false,
  "python.analysis.typeshedPaths": [
    ".venv/Lib/site-packages"
  ],
  "evenBetterToml.schema.associations": {
    // "pyproject.toml": "https://gist.github.com/thakyZ/794efc5003ad437d387f6e75fefcbacc/pyproject.json"
    "pyproject\\.toml": "file:///${workspaceFolder}/.vscode/pyproject.schema.json"
  },
  "pylint.interpreter": [
    "${workspaceFolder}/.env/Scripts/python.exe",
  ],
  "pylint.args": [
    "--disable=C0301,W0611"
  ],
  "pylint.path": [
    "${workspaceFolder}/.env/Scripts/python.exe",
    "-m",
    "pylint"
  ],
  "black-formatter.interpreter": [
    "${workspaceFolder}/.env/Scripts/python.exe",
  ],
  "black-formatter.path": [
    "${workspaceFolder}/.env/Scripts/python.exe",
    "-m",
    "black"
  ],
  "isort.interpreter": [
    "${workspaceFolder}/.env/Scripts/python.exe",
  ],
  "isort.path": [
    "${workspaceFolder}/.env/Scripts/python.exe",
    "-m",
    "isort"
  ],
  "mypy-type-checker.interpreter": [
    "${workspaceFolder}/.env/Scripts/python.exe",
  ],
  "mypy-type-checker.path": [
    "${workspaceFolder}/.env/Scripts/python.exe",
    "-m",
    "mypy"
  ],
}