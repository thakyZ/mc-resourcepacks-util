{
  "python.defaultInterpreterPath": "${workspaceFolder}/.env/Scripts/python.exe",
  "python.analysis.typeCheckingMode": "strict",
  "python.analysis.diagnosticSeverityOverrides": {
    "reportUnusedImport": false
  },
  "python.REPL.enableREPLSmartSend": false,
  "python.poetryPath": "${workspaceFolder}/.env/Scripts/poetry.exe",
  "python.analysis.autoSearchPaths": true,
  "python.envFile": "${workspaceFolder}/.env",
  "python.analysis.indexing": true,
  "python.analysis.diagnosticMode": "workspace",
  "evenBetterToml.schema.associations": {
    "pyproject\\.toml": "https://gist.github.com/thakyZ/794efc5003ad437d387f6e75fefcbacc/pyproject.json",
    // "pyproject\\.toml": "file:///${workspaceFolder}/.vscode/pyproject.schema.json",
  },
  "pylint.interpreter": [
    "${workspaceFolder}/.env/Scripts/python.exe",
  ],
  "pylint.cwd": "${workspaceFolder}/src",
  "pylint.path": [
    "${workspaceFolder}/.env/Scripts/python.exe",
    "-m",
    "poetry",
    "run",
    "pylint",
  ],
  "black-formatter.interpreter": [
    "${workspaceFolder}/.env/Scripts/python.exe",
  ],
  "black-formatter.path": [
    "${workspaceFolder}/.env/Scripts/python.exe",
    "-m",
    "poetry",
    "run",
    "black",
  ],
  "isort.interpreter": [
    "${workspaceFolder}/.env/Scripts/python.exe",
  ],
  "isort.path": [
    "${workspaceFolder}/.env/Scripts/python.exe",
    "-m",
    "poetry",
    "run",
    "isort",
  ],
  "mypy-type-checker.interpreter": [
    "${workspaceFolder}/.env/Scripts/python.exe",
  ],
  "mypy-type-checker.path": [
    "${workspaceFolder}/.env/Scripts/python.exe",
    "-m",
    "poetry",
    "run",
    "mypy",
  ],
  "flake8.interpreter": [
    "${workspaceFolder}/.env/Scripts/python.exe",
  ],
  "flake8.path": [
    "${workspaceFolder}/.env/Scripts/python.exe",
    "-m",
    "poetry",
    "run",
    "flake8",
  ],
  "autopep8.interpreter": [
    "${workspaceFolder}/.env/Scripts/python.exe",
  ],
  "autopep8.path": [
    "${workspaceFolder}/.env/Scripts/python.exe",
    "-m",
    "autopep8",
  ],
}